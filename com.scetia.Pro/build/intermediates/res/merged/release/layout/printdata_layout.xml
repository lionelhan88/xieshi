<com.lessu.xieshi.customView.DragLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#3598DC">
        <LinearLayout
            android:orientation="vertical" android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:clickable="true"
                android:layout_marginTop="25dp"
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_shenqingshangbao"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/shenqingshangbao"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="申请上报"/>
            </LinearLayout>

            <LinearLayout
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_shenhexiazai"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/shenhexiazai"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="审核下载"/>
            </LinearLayout>

            <LinearLayout
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_rukuchakan"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/rukuchakan"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="入库查看"/>
            </LinearLayout>

            <LinearLayout
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_shebeixinxi"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/shebeixinxi"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="设备信息"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="32dp"
                    android:text="功率扫描设置"/>
                <SeekBar
                    android:layout_margin="14dp"
                    android:id="@+id/sb_scan"
                    android:layout_width="165dp"
                    android:layout_height="wrap_content"
                    android:max="25"
                    android:progress="1"
                    style="@style/myseekbarstyle"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="32dp"
                    android:text="弱                        强"/>
            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>


    <com.lessu.xieshi.customView.MyRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#eeeeee"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv_baocun"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="保存"
                    android:textColor="#42b157"
                    android:textSize="14sp"
                    android:layout_marginLeft="16dp"/>
                <TextView
                    android:id="@+id/tv_duqv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="读取"
                    android:textColor="#5979c2"
                    android:textSize="14sp"
                    android:layout_marginLeft="45dp"/>
                <TextView
                    android:id="@+id/tv_qingchu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="清除"
                    android:textColor="#8563aa"
                    android:textSize="14sp"
                    android:layout_marginLeft="45dp"/>
                <TextView
                    android:id="@+id/tv_shujvjiaohu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="数据交互"
                    android:textColor="#df6063"
                    android:textSize="19sp"
                    android:layout_marginLeft="60dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/hengxiana"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_marginLeft="16dp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="已扫描"
                        android:textColor="#1a90cd"
                        android:textSize="14sp"
                        />
                    <TextView
                        android:id="@+id/tv_tiaoma_num"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="？？"
                        android:textColor="#ff6600"
                        android:textSize="14sp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="个条码"
                        android:textColor="#1a90cd"
                        android:textSize="14sp"
                        />

                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="47dp"
                    android:background="#dcdcdc"/>
                <LinearLayout
                    android:layout_marginLeft="16dp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    android:layout_gravity="center_vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="已读取"
                        android:textColor="#1a90cd"
                        android:textSize="14sp"
                        />
                    <TextView
                        android:id="@+id/tv_xinpian_num"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="？？"
                        android:textColor="#ff6600"
                        android:textSize="14sp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="个芯片"
                        android:textColor="#1a90cd"
                        android:textSize="14sp"
                        />

                </LinearLayout>
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/hengxiana"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <com.baoyz.swipemenulistview.SwipeMenuListView
                    android:id="@+id/lv_tiaoma"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent" />
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="#dcdcdc"/>

                <com.baoyz.swipemenulistview.SwipeMenuListView
                    android:id="@+id/lv_xinpian"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"/>
            </LinearLayout>
        </LinearLayout>
    </com.lessu.xieshi.customView.MyRelativeLayout>
</com.lessu.xieshi.customView.DragLayout>