                android:id="@+id/ll_shenqingshangbao"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/shenqingshangbao"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="申请上报"/>
            </LinearLayout>

            <LinearLayout
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_shenhexiazai"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/shenhexiazai"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="审核下载"/>
            </LinearLayout>

            <LinearLayout
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_rukuchakan"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="32dp"
                    android:background="@drawable/rukuchakan"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="17sp"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="16dp"
                    android:text="入库查看"/>
            </LinearLayout>

            <LinearLayout
                android:background="@drawable/sekector_scanitem"
                android:id="@+id/ll_shebeixinxi"
                android:layout_width="match_parent"
                android:layout_height="94dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
