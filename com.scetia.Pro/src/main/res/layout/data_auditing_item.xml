<?xml version="1.0" encoding="utf-8"?>
<com.lessu.xieshi.ListViewCell xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:orientation="horizontal"
	android:gravity="center_vertical"
	android:layout_marginBottom="12dp"
	android:layout_height="wrap_content"
	>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/white">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical"
			android:paddingLeft="23dp"
			>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="45dp"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				>
				<TextView
					android:drawableLeft="@drawable/yuan"
					android:drawablePadding="10dp" 
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="样品编号:"
					android:textSize="17sp"
					android:textColor="#ababab"
					android:gravity="center_vertical"
					/>
				<TextView
					android:id="@+id/sampleIdTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="2013000323"
					android:textSize="17sp"
					android:textColor="#474545"
					android:gravity="center_vertical"
					/>
			</LinearLayout>
			<ImageView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/xuxian"/>
			<LinearLayout
				android:id="@+id/lllll"
				android:layout_width="match_parent"
				android:layout_height="45dp"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				>
				<TextView
					android:drawableLeft="@drawable/yuan"
					android:drawablePadding="10dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="报告编号:"
					android:textSize="17sp"
					android:textColor="#ababab"
					android:gravity="center_vertical"
					/>
				<TextView
					android:id="@+id/reportIdTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="2013004"
					android:textSize="17sp"
					android:textColor="#474545"
					android:gravity="center_vertical"
					/>
			</LinearLayout>
			<ImageView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/xuxian"/>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="45dp"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				>
				<TextView
					android:drawableLeft="@drawable/yuan"
					android:drawablePadding="10dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="报告生成时间:"
					android:textSize="17sp"
					android:textColor="#ababab"
					android:gravity="center_vertical"
					/>
				<TextView
					android:id="@+id/reportTimeTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="2014-09-03"
					android:textSize="17sp"
					android:textColor="#474545"
					android:gravity="center_vertical"
					/>
			</LinearLayout>
			<ImageView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/xuxian"/>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="45dp"
			android:orientation="horizontal"
			android:gravity="center_vertical"
			/>
			<ImageView
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/xuxian"/>
		</LinearLayout>
		<ImageView
			android:id="@+id/selectIcon"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/icon_unchosen"
			android:layout_alignParentRight="true"
			android:layout_marginTop="55dp"
			android:layout_marginRight="20dp"/>

	</RelativeLayout>

</com.lessu.xieshi.ListViewCell>
