<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#ffffff">

    <LinearLayout
        android:background="#ffffff"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="报告编号:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/ReportIDTextView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="杨浦区工地11号"
            android:textColor="#007EFF"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>


    <LinearLayout
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        android:orientation="horizontal"
        android:paddingLeft="10dp">
        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="会员号:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/MemberIdTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="未完工"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>

    <LinearLayout
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="联系人:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/LinkmanTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="房建"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="联系电话:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/TelTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="是否监督抽检:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/IsSuperviserTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">


        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="工程名称:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/ProjectNameTextView"
            android:layout_width="wrap_content"
            android:gravity="center_vertical"
            android:layout_height="wrap_content"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:maxLines="2"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">


        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="建设单位:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/BuildUnitTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="施工单位:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/ConstructionUnitTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="监督单位:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/SupervisorUnitTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="质监站:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/SuperviserKeyTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">

        <TextView
            android:drawableLeft="@drawable/yuan"
            android:drawablePadding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="日期:"
            android:textColor="#b5b5b5"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/ModifyTimeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="杨浦区"
            android:textColor="#4c4a4a"
            android:textSize="17sp" />
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        
        android:orientation="horizontal"
        android:paddingLeft="10dp">
    </LinearLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/xuxian"/>
</LinearLayout>
