<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="#f5f5f5"
	android:padding="20dp"
	android:gravity="center_horizontal">
	<LinearLayout
		android:layout_marginBottom="15dp"
		android:layout_width="match_parent"
		android:layout_height="50dp"
		android:orientation="horizontal"
		android:gravity="center_vertical"
		android:paddingBottom="3dp"
		android:paddingRight="7dp"
		android:paddingLeft="17dp"
		android:background="@drawable/yuanjiaojuxing2"
		>
		<TextView
			android:text="审核类别"
			android:textColor="#ababab"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="17sp"
			/>
		<AbsoluteLayout
			android:layout_width="0dp"
			android:layout_weight="1"
			android:layout_height="match_parent"
			>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				>
				<TextView
					android:layout_marginLeft="10dp"
					android:id="@+id/typeTextView"
					android:text=""
					android:textColor="#333333"
					android:layout_width="0dp"
					android:layout_weight="1"
					android:layout_height="wrap_content"
					android:textSize="17sp"
					/>
				<ImageView
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:src="@drawable/xialacaidan"
					android:scaleType="fitXY"
					/>
			</LinearLayout>
			<Button
				android:id="@+id/typeButton"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@android:color/transparent"
				/>
		</AbsoluteLayout>
	</LinearLayout>
	<LinearLayout
		android:layout_marginBottom="15dp"
		android:layout_width="match_parent"
		android:layout_height="50dp"
		android:orientation="horizontal"
		android:gravity="center_vertical"
		android:paddingBottom="3dp"
		android:paddingRight="7dp"
		android:paddingLeft="17dp"
		android:background="@drawable/yuanjiaojuxing2"
		>
		<TextView
			android:text="审核项目"
			android:textColor="#ababab"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="17sp"
			/>
		<AbsoluteLayout
			android:layout_width="0dp"
			android:layout_weight="1"
			android:layout_height="match_parent"
			>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				>
				<TextView
					android:layout_marginLeft="10dp"
					android:id="@+id/projectTextView"
					android:text=""
					android:textColor="#333333"
					android:layout_width="0dp"
					android:layout_weight="1"
					android:layout_height="wrap_content"
					android:textSize="17sp"
					/>
				<ImageView
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:src="@drawable/xialacaidan"
					android:scaleType="fitXY"
					/>
			</LinearLayout>
			<Button
				android:id="@+id/projectButton"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@android:color/transparent"
				/>
		</AbsoluteLayout>
	</LinearLayout>

	<LinearLayout
		android:layout_marginBottom="45dp"
		android:layout_width="match_parent"
		android:layout_height="50dp"
		android:orientation="horizontal"
		android:gravity="center_vertical"
		android:paddingBottom="3dp"
		android:paddingRight="7dp"
		android:paddingLeft="17dp"
		android:background="@drawable/yuanjiaojuxing2"
		>
		<TextView
			android:text="查询已审核记录"
			android:textColor="#ababab"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="17sp"
			/>
		<AbsoluteLayout
			android:layout_width="0dp"
			android:layout_weight="1"
			android:layout_height="match_parent"
			>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal"
				android:gravity="center_vertical"
				>
				<TextView
					android:layout_marginLeft="10dp"
					android:id="@+id/auditedTextView"
					android:text="否"
					android:textColor="#333333"
					android:layout_width="0dp"
					android:layout_weight="1"
					android:layout_height="wrap_content"
					android:textSize="17sp"
					/>
				<ImageView
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:src="@drawable/xialacaidan"
					android:scaleType="fitXY"
					/>
			</LinearLayout>
			<Button
				android:id="@+id/auditedButton"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@android:color/transparent"
				/>
		</AbsoluteLayout>
	</LinearLayout>

	<Button
		android:id="@+id/searchButton"
		android:layout_marginTop="18dp"
		android:layout_width="260dp"
		android:layout_height="54dp"
		android:text="查  询"
		android:textSize="@dimen/text_size_xlarge"
		android:textColor="#ffffff"
		android:background="@drawable/yuanjiaojuxing1"
		/>

</LinearLayout>
