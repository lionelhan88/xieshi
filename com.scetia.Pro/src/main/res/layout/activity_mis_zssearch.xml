<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_mis_zssearch"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f3f3f3"
    android:orientation="vertical">

    <EditText
        android:id="@+id/et_mis_Certificate_no"
        android:gravity="center"
        android:hint="输入证书编号"
        android:textColorHint="#d7d7d7"
        android:textSize="17sp"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="21dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="52dp"
        android:background="@drawable/pingguxinxichaxun"/>
    
    <Button
        android:id="@+id/bt_miszs_tijiao"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="21dp"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:background="@drawable/shape_tijiaoblue"
        android:text="查 询"
        android:textSize="17sp"
        android:textColor="#ffffff"
        />

</LinearLayout>
