<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout
	android:layout_height="113dp"
	android:layout_width="366dp"
	android:layout_gravity="top"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:paddingTop="8dp">
	>
<RelativeLayout
	android:layout_height="108dp"
	android:layout_width="366dp"
	android:layout_gravity="top"
	android:background="@drawable/yangpinxinxichaxun"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:paddingRight="5dp"
	>

	<LinearLayout android:orientation="vertical"
		android:layout_height="108dp"
		android:layout_width="366dp"
		android:padding="5dp"
		android:layout_gravity="center_horizontal"
		>
		<TextView
			android:maxLines="1"
			android:id="@+id/SampleNameTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="13sp"
			android:textColor="#3d9fe1"
			android:text="asdasdsadsad"/>
		<ImageView
			android:layout_marginTop="3dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/xinxixian"/>
		<LinearLayout
			android:layout_marginTop="3dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="检测状态:"
					/>
				<TextView
					android:id="@+id/Sample_StatusTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#333333"
					android:text="weisongyang"/>
			</LinearLayout>
			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="代表数量:"
					android:paddingLeft="3dp"/>
				<TextView
					android:id="@+id/daibiaoshuliang"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:singleLine="true"
					android:textSize="11sp"
					android:textColor="#333333"
					android:text="weisongyang"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:layout_marginTop="3dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="规格:"
					/>
				<TextView
					android:id="@+id/guige"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#333333"
					android:singleLine="true"
					android:text="weisongyg"/>
			</LinearLayout>
			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="等级/强度:"
					android:paddingLeft="3dp"/>
				<TextView
					android:id="@+id/dengjiqiangdu"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#333333"
					android:singleLine="true"
					android:text="weisongyang"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:layout_marginTop="3dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="工程部位:"
					/>
				<TextView
					android:id="@+id/gongchengbuwei"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#333333"
					android:text="weisongyang"/>
		</LinearLayout>
		<LinearLayout
			android:layout_marginTop="3dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="登记时间:"
					/>
				<TextView
					android:id="@+id/CreateDateTimeTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#333333"
					android:text="weisongyang"/>
			</LinearLayout>
			<LinearLayout
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:orientation="horizontal">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#999999"
					android:text="检测结果:"
					android:paddingLeft="3dp"/>
				<TextView
					android:id="@+id/Exam_ResultTextView"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textSize="11sp"
					android:textColor="#FE4C4D"
					android:singleLine="true"
					android:text="weisongyang"/>
			</LinearLayout>
		</LinearLayout>




	</LinearLayout>
	<ImageView
		android:layout_marginRight="3dp"
		android:layout_centerVertical="true"
		android:layout_alignParentRight="true"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/next11"/>
</RelativeLayout>
</RelativeLayout>