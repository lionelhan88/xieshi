<?xml version="1.0" encoding="utf-8"?>
<com.lessu.xieshi.view.ListViewCell xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:orientation="horizontal"
	android:gravity="center_vertical"
	android:layout_marginBottom="12dp"
	android:layout_height="wrap_content"
	>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:background="@color/white">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:orientation="horizontal"
			>
			<TextView
				android:paddingLeft="20dp"
				android:drawableLeft="@drawable/yuan"
				android:drawablePadding="10dp"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="样品编号:"
				android:textSize="17sp"
				android:textColor="#ababab"
				android:gravity="center_vertical"
				/>
			<TextView
				android:id="@+id/sampleIdTextView"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="2013000323"
				android:textSize="17sp"
				android:textColor="#474545"
				android:gravity="center_vertical"
				/>
		</LinearLayout>
		<ImageView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/xuxian"/>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:orientation="horizontal"
			>
			<TextView
				android:paddingLeft="20dp"
				android:drawableLeft="@drawable/yuan"
				android:drawablePadding="10dp"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="委托编号:"
				android:textSize="17sp"
				android:textColor="#ababab"
				android:gravity="center_vertical"
				/>
			<TextView
				android:id="@+id/conSignIdTextView"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="2013004"
				android:textSize="17sp"
				android:textColor="#474545"
				android:gravity="center_vertical"
				/>
		</LinearLayout>
		<ImageView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/xuxian"/>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:orientation="horizontal"
			>
			<TextView
				android:paddingLeft="20dp"
				android:drawableLeft="@drawable/yuan"
				android:drawablePadding="10dp"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="检测时间:"
				android:textSize="17sp"
				android:textColor="#ababab"
				android:gravity="center_vertical"
				/>
			<TextView
				android:id="@+id/examTimeTextView"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="2014-09-03"
				android:textSize="17sp"
				android:textColor="#474545"
				android:gravity="center_vertical"
				/>
		</LinearLayout>
		<ImageView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/xuxian"/>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:orientation="horizontal"
			>
			<TextView
				android:paddingLeft="20dp"
				android:drawableLeft="@drawable/yuan"
				android:drawablePadding="10dp"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="手机端审核时间:"
				android:textSize="17sp"
				android:textColor="#ababab"
				android:gravity="center_vertical"
				/>
			<TextView
				android:id="@+id/tv_shouchijishenhe"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:text="2014-09-01"
				android:textSize="17sp"
				android:textColor="#474545"
				android:gravity="center_vertical"
				/>
		</LinearLayout>
		<ImageView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/xuxian"/>
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:orientation="horizontal"
			/>
		<ImageView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/xuxian"/>
	</LinearLayout>

</com.lessu.xieshi.view.ListViewCell>