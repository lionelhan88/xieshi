<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LabelView">
        <attr name="label_length" format="dimension" />
        <attr name="label_angle" format="enum">
            <enum name="left_top" value="1" />
            <enum name="right_top" value="2" />
            <enum name="right_bottom" value="3" />
            <enum name="left_bottom" value="4" />
        </attr>
        <attr name="label_background_color" format="color"/>
        <attr name="label_content_text_color" format="color"/>
        <attr name="label_content_text" format="string"/>
        <attr name="label_content_text_size" format="dimension"/>
    </declare-styleable>
</resources>